      if(double){\n\t            digit = digit * 2;\n\t            digit = toSingle(digit);\n\t            double = false;\n\t        } else {\n\t            double = true;\n\t        }\n\t        numArr.push(digit);\n\t    }\n    \n    \n\t    for(i=0; i<numArr.length; i++){\n\t        sumTotal += numArr[i];\n\t    }\n\t    var diff = eval(sumTotal % 10);\n\t    //Inicio BVM-000208 - Canje con DOI \n\t    //console.log(diff);\n\t    //console.log(diff == \"0\");\n\t    //Fin BVM-000208 - Canje con DOI\n\t    return (diff == \"0\");\n\t}\n\n\tfunction toSingle(digit){\n\t    if(digit > 9){\n\t        var tmp = digit.toString();\n\t        var d1 = parseInt(tmp.charAt(0));\n\t        var d2 = parseInt(tmp.charAt(1));\n\t        return (d1 + d2); \n\t    } else {\n\t        return digit;\n\t    }\n\t}\t\n\t\n\t\t//   BVM-000159 \t\n\t   function getFocusNombreAdic() \n       {\n\t    \tdocument.getElementById(\"inptNombreAdici\").focus();\n\t\t\tdocument.getElementById(\"divMensajeNom\").style.display='none';\n\t\t}\n\t\t\n\t\tfunction getFocusNumDoc() \n\t\t{\n\t\t    document.getElementById(\"inptNumDocIdent\").focus();\n\t\t\tdocument.getElementById(\"divMensajeNum\").style.display='none';\n\t\t}\n\t\t\n\t\t\n\t\n\tfunction LimpiarDatos(){\n\t\n\t\t\t//Inicio BVM-000208 - Canjes con DOI\n\t\t\tdocument.getElementById(\"inptFechaVencimiento\").length=0;\t\t\t\n\t\t\t//document.getElementById('txtNumeroCuentaPinpad2').value = \"\";\n\t\t\t$(\"#tipoDocumento\").val(\"\");\n\t\t    $(\"#numeroDocumento\").val(\"\");\n\t\t    $(\"#listaContratosTarjetas\").html(\"\");\n\t\t    document.getElementById('listaContratosTarjetas').disabled = true;\n\t\t\t//Fin BVM-000208 - Canjes con DOI\n\t\t\t//   Inicio BVM-000159 \t\n\t\t\tdocument.getElementById('inptCodGiftcard').value=\"\";\n\t\t\t//   BVM-000159 \t\n\t\t\t// Correccion. Solo eliminar cuando tabla de productos esta vacia:\n\t\t\tvar tableRef = document.getElementById('dataTables-example').getElementsByTagName('tbody')